ASORTEX
{
	if ARRAYSIZE(REQ.ARGS) < 2 {
		LOGGING("asortex: 引数が不足しています")
		-1
		return
	}
	
	_option = REQ.ARGS[0]
	REQ.ARGS[0] = IARRAY()
	_array0 = TOSTR(REQ.ARGS)
	_array1 = IARRAY
	foreach _array0; _ar {
		_array1 ,= _ar
	}

	_array2 = ASORT(_option, _array1)

	_i = 0
	foreach _array2 ; _v {
		LETTONAME("RES.VALUE%(_i)", _v)
		_i++
	}
	// RES.RESULT = ''
	RES.RESULT = ARRAYSIZE(_array2)
}
